{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "paths": {
      "@component/*": ["src/component/*"],
      "@view/*": ["src/view/*"],
      "@constant/*": ["src/constant/*"],
      "@model/*": ["src/model/*"],
      "@service/*": ["src/service/*"],
      "@content/*": ["src/content/*"]
    },
  },
}
