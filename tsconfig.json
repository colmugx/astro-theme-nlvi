{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@component/*": [
        "src/component/*"
      ],
      "@layout/*": [
        "src/layout/*"
      ],
      "@constant/*": [
        "src/constant/*"
      ],
      "@model/*": [
        "src/model/*"
      ],
      "@service/*": [
        "src/service/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@util/*": [
        "src/util/*"
      ]
    },
  }
}